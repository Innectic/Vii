
print :: (message: string) {
    native_print(message)
}

print :: (message: char) {
    native_print(message)
}

print :: (message: int) {
    native_print(message)
}

print :: (message: float) {
    native_print(message)
}

print :: (state: bool) {
	if state == true {
		native_print("true")
	} else {
		native_print("false")
	}
}
